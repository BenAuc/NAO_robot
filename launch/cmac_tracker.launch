<?xml version="1.0"?>
<launch>
    <!--include other launch files-->
    <include file="$(find nao_bringup)/launch/nao_full_py.launch">
    </include>

    <include file="$(find nao_apps)/launch/tactile.launch">
    </include>

    <!-- Load parameters from yaml files containing waypoints to parameter server-->
    <rosparam command="load"
        file="$(find tutorial_4)/data/config/joint_limits.yaml"/>

    <!--launch cmac_tracker node-->
    <node name="CMACTrackerNode" pkg="tutorial_4" type="cmac_blob_tracker_node.py"
	output="screen" respawn="false"/>

</launch>
